{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "lib": [
      "dom",
      "es2015"
    ],
    "moduleResolution": "node",
    "declaration": true,
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "../../dist/e2e"
  },
  "angularCompilerOptions": {
    "skipMetadataEmit": true,
    "outDir": "../../dist/e2e"
  },
  "include": [
    "test/**/entry.ts",
    "test/**/AppModule.ts"
  ]
}